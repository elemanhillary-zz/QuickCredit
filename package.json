{
    "name": "quickcredit-v2",
    "version": "1.0.0",
    "description": "quick credit api version 2 using postgresql",
    "main": "src/index.js",
    "scripts": {
      "build": "babel src -d dist --source-maps",
      "serve": "node dist/index.js",
      "start": "cross-env ./node_modules/.bin/babel-node src/index.js",
      "start:dev": "cross-env ./node_modules/.bin/nodemon src/index.js --exec babel-node",
      "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\""
    },
    "engines": {
      "node": ">=8"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/elemanhillary/QuickCreditV2.git"
    },
    "keywords": [
      "QuickCredit",
      "Version",
      "2",
      "PostgreSQL"
    ],
    "author": "eleman hillary barnabas",
    "license": "ISC",
    "bugs": {
      "url": "https://github.com/elemanhillary/QuickCreditV2/issues"
    },
    "homepage": "https://github.com/elemanhillary/QuickCreditV2#readme",
    "dependencies": {
      "@babel/cli": "^7.4.4",
      "@babel/core": "^7.4.4",
      "@babel/node": "^7.2.2",
      "@babel/polyfill": "^7.4.4",
      "@babel/preset-env": "^7.4.4",
      "@hapi/joi": "^15.0.0",
      "acorn": "^6.1.1",
      "bcrypt": "^3.0.6",
      "body-parser": "^1.19.0",
      "dotenv": "^8.0.0",
      "eslint-plugin-jest": "^22.5.1",
      "eslint-plugin-node": "^9.0.1",
      "express": "^4.16.4",
      "jest": "^24.8.0",
      "jest-cli": "^24.8.0",
      "jsonwebtoken": "^8.5.1",
      "make-runnable": "^1.3.6",
      "nodemon": "^1.19.0",
      "pg": "^7.11.0",
      "prettier": "^1.17.1"
    },
    "devDependencies": {
      "cross-env": "^5.2.0",
      "eslint": "^5.16.0",
      "eslint-config-airbnb-base": "^13.1.0",
      "eslint-plugin-import": "^2.17.2"
    }
}
  