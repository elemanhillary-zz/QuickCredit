{
  "name": "quickcredit-v2",
  "version": "1.0.0",
  "description": "quick credit api version 2 using postgresql",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist --source-maps",
    "test": "nyc --reporter=html --reporter=text mocha --require @babel/register   --timeout 100000 ./tests --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "showcoverage": "start ./coverage/index.html",
    "tables": "cross-env ./node_modules/.bin/babel-node ./model/QuickCredit_Db.js",
    "serve": "node ./index.js",
    "start": "cross-env ./node_modules/.bin/babel-node src/index.js",
    "start:dev": "cross-env ./node_modules/.bin/nodemon src/index.js --exec babel-node",
    "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elemanhillary/QuickCredit.git"
  },
  "keywords": [
    "QuickCredit",
    "Version",
    "2",
    "PostgreSQL"
  ],
  "author": "eleman hillary barnabas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/elemanhillary/QuickCredit/issues"
  },
  "homepage": "https://github.com/elemanhillary/QuickCredit#readme",
  "dependencies": {
    "@hapi/joi": "^15.0.0",
    "acorn": "^6.1.1",
    "babelify": "^10.0.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "browserify": "^16.2.3",
    "cors": "^2.8.5",
    "coveralls": "^3.0.3",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "eslint-plugin-jest": "^22.5.1",
    "eslint-plugin-node": "^9.0.1",
    "express": "^4.16.4",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.1",
    "pg": "^7.11.0",
    "pg-format": "^1.0.4",
    "prettier": "^1.17.1",
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "@babel/register": "^7.4.4",
    "@babel/polyfill": "^7.4.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2"
  }
}
